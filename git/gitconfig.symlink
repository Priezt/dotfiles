[user]
	name = Priezt
	email = vegetabird@gmail.com

[alias]
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative --all
	ci = commit

[color]
	ui = true
	status = auto
	branch = auto
	ui = auto

[help]
	autocorrect = 1

[core]
	excludesfile = ~/.gitignore
	editor = vim

[github]
	user = Priezt

[push]
	default = matching
