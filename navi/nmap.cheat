% nmap

# scan vulnerability
nmap -sS -sV --script=vulscan/vulscan.nse -p <ports> <host>
nmap -sS -sV --script=vulners -p <ports> <host>

# install vulnerability db
cd /usr/share/nmap/scripts && git clone https://github.com/vulnersCom/nmap-vulners.git && cp /usr/share/nmap/scripts/nmap-vulners/vulners.nse /usr/share/nmap/scripts/
cd /usr/share/nmap/scripts && git clone https://github.com/scipag/vulscan.git

# update script db
nmap --script-updatedb


